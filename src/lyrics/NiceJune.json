        {
            "time": "03:38.00",
            "timeFormat": 218.0,
            "lyric": "溶ける飛行機雲",
            "trans": "轨迹云逐渐消散"
        },
        {
            "time": "03:42.81",
            "timeFormat": 222.81,
            "lyric": "がなる現世の境界で",
            "trans": "立于喧嚣尘世边境"
        },
        {
            "time": "03:47.18",
            "timeFormat": 227.18,
            "lyric": "愚かなあなたは泣いていた",
            "trans": "无知的你泣不成声"
        },
        {
            "time": "03:51.73",
            "timeFormat": 231.73,
            "lyric": "薫る夏風に誘われて",
