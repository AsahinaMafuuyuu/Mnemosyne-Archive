        },
        {
            "time": "03:56.38",
            "timeFormat": 236.38,
            "lyric": "悲しくなどないさ",
            "trans": "无需感觉任何悲伤"
        },
        {
            "time": "04:00.87",
            "timeFormat": 240.87,
            "lyric": "天国も地獄も無いのなら",
            "trans": "若既无天堂也无地狱"
        },
        {
            "time": "04:03.60",
            "timeFormat": 243.6,
            "lyric": "こんな泥塗れの現実を",
            "trans": "那又该由谁来审判"
        },
        {
            "time": "04:06.62",
            "timeFormat": 246.62,
            "lyric": "誰が裁けるの",
            "trans": "这泥泞不堪的现实"
        },
        {
            "time": "04:08.02",
            "timeFormat": 248.02,
            "lyric": "透過 「また会いましょう」",
            "trans": "透过“后会有期”"
        }
    ]
}